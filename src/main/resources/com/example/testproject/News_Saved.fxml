<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="522.0" prefWidth="655.0" style="-fx-background-color: #dcf0fa;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testproject.SavedNewsController">
   <children>
      <Pane prefHeight="69.0" prefWidth="655.0" style="-fx-background-color: black;">
         <children>
            <Label layoutX="14.0" layoutY="15.0" text="Saved News Articles" textFill="#a5b5e8">
               <font>
                  <Font name="Times New Roman Bold" size="36.0" />
               </font>
            </Label>
            <Button fx:id="home" layoutX="523.0" layoutY="21.0" mnemonicParsing="false" onAction="#onHomeButtonOnClick" prefHeight="28.0" prefWidth="84.0" style="-fx-background-color: #b01058;" text="       Home">
               <font>
                  <Font name="Times New Roman Bold" size="14.0" />
               </font>
            </Button>
            <ImageView fx:id="homeImage" fitHeight="25.0" fitWidth="25.0" layoutX="530.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/8666691_home_icon.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <ListView fx:id="savedNewsListView" layoutX="12.0" layoutY="82.0" prefHeight="380.0" prefWidth="627.0" />
      <Button fx:id="delete" layoutX="537.0" layoutY="480.0" mnemonicParsing="false" onAction="#onDeleteButtonOnClick" prefHeight="28.0" prefWidth="84.0" style="-fx-background-color: #950F1D;" text="Delete">
         <font>
            <Font name="Eras Bold ITC" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
